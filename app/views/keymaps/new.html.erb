<%# KEYMAPS / NEW %>
<div class="row mt-4">
  <div class="col-12">
    <h3>New Keymap</h3>
    <%# DISPLAY ERROR %>
    <%= render 'layouts/errors', article: @keymap %>
    <%# FORM %>
    <%= form_with scope: :keymap, url: keymaps_path, method: :post, local: true do |f| %>
      <fieldset>
        <%= render 'keymaps/form', f: f %>
        <%= f.button 'Submit', type: 'submit', onclick: 'submit()', id: 'customButton', class: "btn btn-primary"%>
      </fieldset>
    <% end %>
  </div>
</div>
